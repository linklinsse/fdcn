; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="fdcn"
config/description="Application de suivi de progression dans le livre La Forteresse du Chaudron Noir"
run/main_scene="res://scenes/main.tscn"
boot_splash/image="res://images/barbare_icon.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://images/fdcn_icon.png"

[autoload]

Sounder="*res://scripts/auto_loads/sounder.tscn"
Utils="*res://scripts/auto_loads/utils.gd"
BookData="*res://scripts/auto_loads/book_data.gd"
AppParameters="*res://scripts/auto_loads/parameters.gd"
Swiper="*res://scripts/auto_loads/swipe.gd"
Player="*res://scripts/auto_loads/player.gd"
SceneSwitcher="*res://scripts/auto_loads/scene_switcher.gd"

[display]

window/size/width=720
window/size/height=1080
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

touch=false
driver=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[logging]

file_logging/enable_file_logging=true
file_logging/max_log_files=1

[network]

limits/debugger_stdout/max_chars_per_second=65536

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
environment/default_environment="res://default_env.tres"
